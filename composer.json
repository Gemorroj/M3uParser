{
    "name": "gemorroj/m3u-parser",
    "description": "m3u playlist parser/generator",
    "keywords": ["m3u", "m3u8", "playlist"],
    "type": "library",
    "license": "LGPL-3.0-or-later",
    "authors": [
        {
            "name": "Gemorroj"
        }
    ],
    "require": {
        "php": ">=8.0.2"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.6.22",
        "friendsofphp/php-cs-fixer": "^3.73.1",
        "phpstan/phpstan": "^2.1"
    },
    "autoload": {
        "psr-4": {
            "M3uParser\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "M3uParser\\Tests\\": "tests/"
        }
    }
}
